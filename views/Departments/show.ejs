    <h1>Welcome to Department Stores</h1>
    <section>
        <h3>Products</h3>
        <ul>
            <% Dept.products.forEach(product => { %>
                <li>
                    <a href="/Store/Products/<%=product._id %>"><%= product.name %></a> <br/>
                   
                </li>
            <% }) %>
        </ul>
        <a href="/Store/Depts/<%=Dept._id %>/edit">Edit Department Info</a>
        <form action="/Store/Depts/<%=Dept._id%>?_method=DELETE" method="POST"><input type="submit" value="delete department"></form>
    </section>
